{
   "suggest.noselect": false,
   "suggest.acceptSuggestionOnEnter": "on",
   "coc.preferences.formatOnSaveFiletypes": 
   [
      "javascript",
      "typescript",
      "json",
      "javascriptreact",
      "typescriptreact",
      "html",
      "css",
      "yaml",
      "python"
   ],
   "python.formatting.provider": "black",
   "python.formatting.blackPath": "~/.local/bin/black",
   "clangd.path": "/home/duckyshine04/.config/coc/extensions/coc-clangd-data/install/17.0.3/clangd_17.0.3/bin/clangd",
   "clangd.arguments": ["--background-index", "-I/usr/include/c++/11","-I/usr/include/x86_64-linux-gnu/c++/11"],
   "languageserver": {
      "clangd": {
         "command": "clangd",
         "rootPatterns": ["compile_commands.json", ".vim/", ".git/", ".hg/"],
         "filetypes": ["c", "cpp", "objc", "objcpp"]
      },
      "tsserver": {
         "module": "typescript-language-server",
         "args": ["--stdio"],
         "filetypes": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
         "initializationOptions": {
            "preferences": {
               "importModuleSpecifier": "relative"
            }
         }
      },
      "eslint": {
         "module": "eslint-language-server",
         "args": ["--stdio"],
         "filetypes": ["javascript", "javascriptreact", "typescript", "typescriptreact"]
      }
   }
}


